{
    "name": "feedback-popup",
    "version": "2.0.0-rc1",
    "description": "A simple to use popup for collecting feedback from users about the sites that they are using. Currently it captures a screenshot of the users browser, the users OS and browser name + version and also a message from the user. This is all then sent to an API, where you can do whatever you like with the information.",
    "private": false,
    "scripts": {
        "prepublish": "node_modules/babel-cli/bin/babel.js src --out-dir lib",
        "build": "nwb build-web-app",
        "clean": "nwb clean-app",
        "start": "nwb serve-web-app --reload"
    },
    "babel": {
        "presets": [
            "es2015"
        ]
    },
    "dependencies": {
        "axios": "^0.18.0",
        "html2canvas": "^1.0.0-alpha.12",
        "platform": "^1.3.5"
    },
    "devDependencies": {
        "autoprefixer": "^9.4.10",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-preset-es2015": "^6.24.1",
        "browser-sync": "^2.26.3",
        "cssnano": "^4.1.10",
        "eslint": "^4.19.1",
        "eslint-plugin-react": "^7.7.0",
        "gulp": "^3.9.1",
        "gulp-plumber": "^1.2.1",
        "gulp-postcss": "^8.0.0",
        "gulp-sass": "^4.0.1",
        "gulp-sourcemaps": "^2.6.5",
        "gulp-watch": "^5.0.0",
        "nwb": "0.21.x",
        "postcss-import": "^12.0.1",
        "precss": "^4.0.0"
    },
    "author": "Tommy Scribble",
    "email": "tommy@scribbledesign.co.uk",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/in-touch-dev/feedback-popup"
    },
    "homepage": "https://github.com/in-touch-dev/feedback-popup#readme",
    "main": "lib/js/index.js",
    "keywords": [
        "feedback",
        "popup",
        "widget",
        "screenshot",
        "platform",
        "html2canvas"
    ]
}
